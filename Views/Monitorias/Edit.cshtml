@model MonitoriaAgentes.Models.Monitoria

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>Monitoria</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="MonitoriaId" />
            <div class="form-group">
                <label asp-for="Assunto" class="control-label"></label>
                <input asp-for="Assunto" class="form-control" />
                <span asp-validation-for="Assunto" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DataDaMonitoria" class="control-label"></label>
                <input asp-for="DataDaMonitoria" class="form-control" />
                <span asp-validation-for="DataDaMonitoria" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NomeAgente" class="control-label"></label>
                <input asp-for="NomeAgente" class="form-control" />
                <span asp-validation-for="NomeAgente" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="MatriculaAgente" class="control-label"></label>
                <input asp-for="MatriculaAgente" class="form-control" />
                <span asp-validation-for="MatriculaAgente" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AD_Agente" class="control-label"></label>
                <input asp-for="AD_Agente" class="form-control" />
                <span asp-validation-for="AD_Agente" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NomeSupervisor" class="control-label"></label>
                <input asp-for="NomeSupervisor" class="form-control" />
                <span asp-validation-for="NomeSupervisor" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Descricao" class="control-label"></label>
                <input asp-for="Descricao" class="form-control" />
                <span asp-validation-for="Descricao" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Voltar</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
